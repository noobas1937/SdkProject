<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/net_ping_layout"
    android:orientation="vertical"
    android:layout_gravity="center_vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <FrameLayout
        android:id="@+id/cs_confirm_net"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <LinearLayout
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/net_bg"
        android:paddingTop="5dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:paddingBottom="5dp"
        android:gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/net_title"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="chooseServer"
            android:textColor="@color/cs__white"
            android:gravity="center"
            android:background="@color/net_title_bg"
            />
        <ListView
            android:id="@+id/net_ping_list"
            android:layout_below="@+id/net_title"
            android:gravity="center"
            android:divider="@color/net_title_bg"
            android:dividerHeight="1px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </ListView>
    </LinearLayout>


</RelativeLayout>